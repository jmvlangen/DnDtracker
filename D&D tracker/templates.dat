        stealthBonus:variable( (DEXbonus+(stealthProficiency*proficiencyBonus)) )
        CONsaveBonus:variable( (CONbonus+(CONsaveProficiency*proficiencyBonus)) )
        INTsaveBonus:variable( (INTbonus+(INTsaveProficiency*proficiencyBonus)) )
        arcanaBonus:variable( (INTbonus+(arcanaProficiency*proficiencyBonus)) )
        historyBonus:variable( (INTbonus+(historyProficiency*proficiencyBonus)) )
        investigationBonus:variable( (INTbonus+(investigationProficiency*proficiencyBonus)) )
        natureBonus:variable( (INTbonus+(natureProficiency*proficiencyBonus)) )
        religionBonus:variable( (INTbonus+(religionProficiency*proficiencyBonus)) )
        WISsaveBonus:variable( (WISbonus+(WISsaveProficiency*proficiencyBonus)) )
        animalHandlingBonus:variable( (WISbonus+(animalHandlingProficiency*proficiencyBonus)) )
