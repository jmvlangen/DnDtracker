        insightProficiency:variable( 0 )
        medicineProficiency:variable( 0 )
        perceptionProficiency:variable( 0 )
        survivalProficiency:variable( 0 )
        CHAsaveProficiency:variable( 0 )
        deceptionProficiency:variable( 0 )
