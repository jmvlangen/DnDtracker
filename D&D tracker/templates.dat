collection(
    item:collection(
        _value:variable( print((name+if(magical," (magical)")),("value: "+value),description) )
        _type:variable( "item" )
        magical:variable( $FALSE )
        description:variable( "<no description>" )
        value:variable( "unknown" )
    )
    character:collection(
        DEXbonus:variable( ((DEX-10)/2) )
