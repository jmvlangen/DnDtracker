        medicineBonus:variable( (WISbonus+(medicineProficiency*proficiencyBonus)) )
        perceptionBonus:variable( (WISbonus+(perceptionProficiency*proficiencyBonus)) )
        survivalBonus:variable( (WISbonus+(survivalProficiency*proficiencyBonus)) )
        CHAsaveBonus:variable( (CHAbonus+(CHAsaveProficiency*proficiencyBonus)) )
        deceptionBonus:variable( (CHAbonus+(deceptionProficiency*proficiencyBonus)) )
        intimidationBonus:variable( (CHAbonus+(intimidationProficiency*proficiencyBonus)) )
